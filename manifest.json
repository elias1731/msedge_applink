{
  "manifest_version": 3,
  "name": "AppLink",
  "version": "1.0.0",
  "description": "Leitet bestimmte Webseiten automatisch an festgelegte App-URIs weiter.",
  "permissions": [
    "webNavigation",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "AppLink konfigurieren"
  }
}

