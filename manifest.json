{
  "manifest_version": 3,
  "name": "AppLink",
  "version": "2.0.0",
  "description": "Automatically redirects Spotify, YouTube, and YT Music links to their respective native apps or LiveContainer.",
  "permissions": [
    "webNavigation",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "AppLink Configuration"
  }
}